{
  "name": "vr-chain",
  "version": "1.0.0",
  "scripts": {
    "install": "npm rebuild",
    "start": "budo src/index.js --live --verbose --port 3000 --open",
    "server": "node run_server.js",
    "test-server": "./node_modules/tape/bin/tape src/server/*_test.js | node_modules/.bin/tap-spec",
    "test": "./node_modules/browserify/bin/cmd.js src/*_test.js | ./node_modules/tape-run/bin/run.js",
    "smoke": "./node_modules/tape/bin/tape smoke_test.js | node_modules/.bin/tap-spec",
    "browserify": "./node_modules/browserify/bin/cmd.js -r ./src/index.js:main -o bundle.js"
  },
  "license": "ISC",
  "devDependencies": {
    "browserify": "^14.0.0",
    "budo": "^9.4.7",
    "nightmare": "^2.10.0",
    "request-promise-native": "^1.0.3",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "tape-run": "^2.1.5",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "cors": "^2.8.1",
    "express": "^4.14.1"
  }
}
